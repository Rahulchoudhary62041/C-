#include <iostream>
using namespace std;
int main(){
    int product;
    int counter=1;
    int number;
    cout<<"Enter the number to print the table: ";
    cin>>number;
    in:
    product = number*counter;
    cout<<number<<" x "<<counter<<" = "<<product<<endl;
    counter = counter + 1;
    if (counter <= 10){
        goto in;
    }
    return 0;
}
